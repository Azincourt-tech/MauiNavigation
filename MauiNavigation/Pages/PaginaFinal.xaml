                Clicked="button1_Clicked"/>

        <Button Text="Ir para a Página Inicial" 
                FontSize="20"
                x:Name="button2"
                BackgroundColor="Black"
                Clicked="button2_Clicked" />

    </VerticalStackLayout>
</ContentPage>