             x:Class="MauiNavigation.Pages.PaginaFinal"
             Title="Pagina Final">

    <VerticalStackLayout VerticalOptions="Center" Padding="30" Spacing="30" >

        <Button Text="Ir para a Página 2" 
                FontSize="20"
